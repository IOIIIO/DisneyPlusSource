<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto">
    <item android:state_activated="true">
        <objectAnimator android:duration="@integer/duration2" android:valueTo="1" android:valueType="floatType" android:propertyName="alpha"/>
    </item>
    <item android:state_activated="false">
        <objectAnimator android:duration="@integer/duration2" android:valueTo="0" android:valueType="floatType" android:propertyName="alpha"/>
    </item>
</selector>
